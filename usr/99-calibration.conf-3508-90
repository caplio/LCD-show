Section "InputClass"
        Identifier      "calibration"
        MatchProduct    "ADS7846 Touchscreen"
        Option  "Calibration"   "3945 233 3939 183"
        Option  "SwapAxes"      "1"
        
        #SwapX,Y and inverting X,Y for 90
        Option "TransformationMatrix" "0 1 0 -1 0 1 0 0 1"
EndSection
